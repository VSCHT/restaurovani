  /* form */

  .predmety__form__h {
    font-family: Noto_Sans_regular;
    font-size: 35px;
  }
  .predmety__form__div__label {
    font-family: Noto_Sans_regular;
    font-size: 25px;
    gap: 10px;
  }

  .predmety__form__div__radio {
    appearance: none;
    background-color: #fff;
    margin: 0;
    color: #333;
    width: 35px;
    height: 35px;
    border: 2px solid #333;
    border-radius: 50%;
    display: grid;
    place-content: center;
  }

  .predmety__form__div__input {
    font-family: Noto_Sans_regular;
    width: calc(100% - 20px);
    height: 35px;
    border: solid #555 1px;
    border-radius: 40px;
    padding: 0px 10px;
  }
  

  .predmety__form__div {
    width: calc(50vw - 30px);
    gap: 20px;
  }
  .predmety__form__div-unit{
    width: calc(20vw - 30px);
  }
  .predmety__form {
    align-items: center;
    gap: 25px;
  }
  .predmety__form__div__input-radio {
    justify-content: space-between;
  }

  .predmety__form__div__radio {
    appearance: none;
    background-color: #fff;
    margin: 0;
    color: #333;
    width: 20px;
    height: 20px;
    border: 2px solid #333;
    border-radius: 50%;
    display: grid;
    place-content: center;
  }

  .predmety__form-main {
    margin: 0px 0px 5px;
    gap: 20px;
  }
  .form.main-page__btn__addPredmety {
    width: 250px;
    font-size: 25px;
  }
  .predmety__form__div__radio::before {
    content: "";
    width: 20px;
    height: 20px;
    border-radius: 50%;
    transform: scale(0);
    background-color: #e0472b;
  }
  .predmety__form__div__radio:checked::before {
    transform: scale(1);
  }

  /* details page */
  /* bg */
  .details__bg {
    background-image: url("../../../../static/images/bgDetails.png");

    height: auto;

    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    padding-top: 100px;
  }

  .details__container {
    background-color: white;
    height: auto;
    width: 80vw;
    margin: 0 auto;
    justify-content: center;
    align-items: center;
    z-index: -1;
    padding: 60px;
  }

  /* header */

  .details__div__header {
    background-color: #de4929;
    height: 80px;
    width: 77vw;
    color: white;
    justify-content: space-between;
    padding-left: 30px;
    margin: 0 auto;
    border-radius: 40px;
  }
  .details__div__header-additional {
    visibility: hidden;
    position: absolute;
  }
  .details__div__vr {
    width: 1px;
    border-right: 1px solid white;
    height: 20px;
    margin: 0px 10px;
  }
  .details__div__header-h {
    word-wrap: break-word;
    letter-spacing: 1px;
    font-family: Noto_Sans_regular;
    font-size: 50px;
  }

  /* photos */

  .details_div_images-container {
    width: 80vw;
    overflow: hidden;
  }
  .details_div_images-container__btn-img {
    width: 60px;
  }
  .details_div_images-container__btn-left {
    position: absolute;
    left: 90px;
  }
  .details_div_images-container__btn-right {
    position: absolute;
    right: 90px;
  }
  .details_div_images {
    margin-top: 50px;
    margin-bottom: 50px;
    width: 140vw;
  }

  .predmety__card__img-container.details img {
    width: 270px;
    height: 380px;
  }

  /* info */
  .details__div__details {
    justify-content: space-between;
    height: 400px;
    width: 70vw;
    margin: 0 auto;
    flex-direction: column;
    align-items: flex-start;
  }

  .details__div__details-box {
    width: 700px;
    height: 60px;
    gap: 60px;
    flex-direction: row;
  }
  .details__div__details-box-smaller {
    width: 280px;
    height: 50px;
  }
  .details__div__details-box-smaller:nth-child(even) {
    width: 900px;
    height: 50px;
  }
  .details__div__info {
    width: 750px;
    margin: 0 auto;
    flex-direction: column;
  }
  .details__div__info-left {
    width: 750px;
    order: 1;
  }

  .details__div__info-left table tr td:nth-child(odd) {
    font-family: Noto_Sans_semibold;
    width: 180px;
    position: relative;
  }
  .details__div__info-left table tr td {
    height: 130px;
    line-height: 35px;
    vertical-align: top;
  }
  .details__div__info-right {
    width: 750px;
  }
  .details__div__info-right.parag {
    font-family: Noto_Sans_semibold;
    margin-bottom: 30px;
  }

  .details__div__info-right__text {
    width: 750px;
    height: 350px;
  }

  .details__div__desc {
    height: 300px;
    width: 750px;
  }
  .details__div__desc p {
    margin-top: 60px;
  }
  /* docs */
  .details__div__docs {
    width: 70vw;
    margin: 0 auto;
    justify-content: space-between;
    margin-top: 30px;
  }

  a {
    color: #222;
  }
  .details__div__docs p:first-child {
    font-family: Noto_Sans_semibold;
  }

  .details__div__docs-img {
    width: 25px;
    height: auto;
    margin: 0px 10px;
  }
  .details__div__docs-files {
    flex-direction: column;
    gap: 15px;
    align-items: flex-start;
  }

  /* additional */
  .parag {
    line-height: 28px;
    word-wrap: break-word;
    font-family: Noto_Sans_regular;
    font-size: 20px;
  }
  .vr-small {
    visibility: hidden;
    position: absolute;
  }
  .vr-huge-horiz {
    width: 80vw;
    border-top: 1px solid black;
    height: 1px;
    margin: 0 auto;
    margin-bottom: 30px;
    margin-top: 30px;
  }
  .vr-huge-vert {
    position: absolute;
    visibility: hidden;
  }
  .vr-huge-vert-mobil {
    width: 1px;
    border-right: 1px solid black;
    height: 470px;
  }
}
