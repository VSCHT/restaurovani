@font-face {
  font-family: Noto_Sans_light;
  src: url("../../../fonts/Source_Sans_Pro/SourceSansPro-Light.ttf");
}
@font-face {
  font-family: Noto_Sans_semibold;
  src: url("../../../fonts/Source_Sans_Pro/SourceSansPro-Bold.ttf");
}
@font-face {
  font-family: Noto_Sans_regular;
  src: url("../../../fonts/Source_Sans_Pro/SourceSansPro-Regular.ttf");
}
@font-face {
  font-family: Noto_Sans_bold;
  src: url("../../../fonts/Source_Sans_Pro/SourceSansPro-Bold.ttf");
}
.horiz-div {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.vert-div {
  display: flex;
  flex-direction: column;
}


h3,
h2,
h4 {
  margin: 0px;
}
@media (min-width: 1200px) {
    body,
  html {
    margin: 0;
    padding: 0;
    height: 100%;
  }
  .main-page__menu,
  .vsht-logo.main-page__div__vsht-logo,
  .main-page__title,
  .actions-mainPage {
    z-index: 1;
  }

  /* all logos */
  .div__vsht-logo {
    display: flex;
    flex-direction: row;
    align-items: center;
    z-index: 10 !important;
    position: relative;
    width: 300px;
  }
  .text__vsht-logo {
    font-size: 11px;
    font-family: Noto_Sans_bold;
    width: 15vw;
  }
  .image__vsht-logo,
  .text__vsht-logo {
    max-width: 100%;
    height: auto;
  }

  .image__vsht-logo {
    width: 40px;
    margin-right: 10px;
    
  }

  /* main page */
  /* bg */
  .main-page__half--background {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    z-index: -1;
    top: 0
  }
  .main-page__half--background.bg-top {
    background-color: #EAEAE2;
    top: 0px;
    left: 0px;
    height: 60%;
    width: 100%;

    background-size: cover;
    align-items: center;
    justify-content: center;
    background-position: center center;
  }
  .main-page__half--background.bg-bottom {
    background-color: white;
    width: 100%;
    height: 40%;
    top: 60%;
  }
  /* menu */
  .main-page__menu {
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    background-color: #9b9b93;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10px 10px;
    z-index: 1000;
     min-height:30px
  }
  .btn {
    display: flex;
    align-items: center;
    background: none;
    border: none;
    color: white;
    font-size: 18px;
    cursor: pointer;
    font-weight: 200;
    font-family: Noto_Sans_regular;
  }

  .main-page__menu__btn {
    margin-left: 10px;
    font-size: 17px;
    font-family: Noto_Sans_regular;
  }

  .btn.main-page__menu__btn:first-child {
    margin-left: 10vw;
  }
  .main-page__menu__btn img {
    margin-right: 7px;
  }

  .main-page__menu__btn.burger {
    visibility: hidden;
    position: absolute;
  }
  /* .btn.main-page__menu__btn:not(:first-child) {
    margin-right: 10vw;
  } */
  .main-page__menu__btn-group {
    gap: 15px;
    margin-right: 10vw
  }

  /* main page logo */
  .main-page__div__vsht-logo {
    position: relative;
    top: 100px;
    left: 17vw;
  }
  .vert-div.vsht-logo {
    padding-left: 200px;
  }

  /* main content */
  .main-page__main-container {
    display: flex;
    position: relative;
    top: 180px;
    width: 80vw;
    height: 70vh;
    margin: 0 auto
  }
  /* right side of main content */

  .main-page__main-container__image {
    width: 40vw;
    height: 100%;
  }

  .main-page__div__main-image {
    flex-grow: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
  }

  .main-page__main-image img {
    max-width: 100%;
    max-height: 100%;
    width: auto;
    height: auto;
  }
  .main-page__div__main-image__div-arrows{
    width: 80px;
    height: auto;
    position: relative;
    right: 0px;
    bottom: 0px
  }

  /* left side of main content */
  .main-page__main-container__content {
    width: 40vw;
    height: 100%;
    justify-content: space-between;
    padding-right: 50px
  }
  .main-page__title {
    position: relative;
    font-size: 55px;
    gap: 20px;
    font-family: Noto_Sans_light;
    line-height: 60px;
    word-wrap: break-word;
    color: #74756e;
    margin-right: 50px;
    align-items: flex-start
  }
  .main-page__title img{
    margin-top: 10px
  }
  .main-page__text__copyright {
    color: #73746f;
    font-size: 11px;
    font-family: Noto_Sans_regular;
    line-height: 19px;
    word-wrap: break-word;
  }

  /* page actions on the left side */
  .main-page__div__btn {
    position: relative;
    display: flex;
    flex-direction: column;
    gap: 20px;
    margin-left: 7vw;
  }
  .main-page__btn__addPredmety {
    color: white;
    background-color: #de4929;
    border-radius: 32px;
    height: 45px;
    width: 18vw;
    border: none;
     font-family: Noto_Sans_regular;
      
  }

  .main-page__btn__addPredmety:hover {
    background-color: #e76c50;
  }
  .main-page__input-search {
    border-radius: 32px;
    height: 30px;
    width: 18vw ;
    border: solid 1px #9b9b93;
    padding: 0px 15px;
    font-family: Noto_Sans_regular;
  }

  /* predmety page */

  /* bg */

  .predmety__body-bg {
    background-color: #eaeae2;
    width: 100vw
  }

  /* menu part */

  .dropbtn {
    background-color: #9b9b93;
    color: white;
    padding: 16px;
    font-size: 16px;
    font-family: Noto_Sans_regular;
    border: none;
  }

  .menu__div__dropdown {
    position: relative;
    display: inline-block;
  }

  .predmety__menu__btn.profil img {
    visibility: hidden;
    position: absolute;
  }
  .menu__profil-img {
    margin: 0px 10px;
  }
  .menu__dropdown__content {
    display: none;
    position: absolute;
    background-color: #f1f1f1;
    min-width: 160px;
    box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
    z-index: 1;
    font-family: Noto_Sans_regular;
  }

  .menu__dropdown__content a {
    color: black;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
  }

  .predmety__menu__btn.profil {
    position: absolute;
    visibility: hidden;
  }
  .btn.menu__btn {
    font-size: 16px;
    cursor: pointer;
    font-family: Noto_Sans_regular;
    margin-right: 10vw;
  }

  .menu__dropdown__content a:hover {
    background-color: #ddd;
  }

  .menu__div__dropdown:hover .menu__dropdown__content {
    display: block;
  }

  .menu__div__dropdown:hover .btn.menu__btn {
    background-color: #9b9b93;
  }

  /* main part--cards */

  .predmety_main-container {
    width: 53vw;
    position: relative;
    top: 180px;
    left: 15vw;
  }
  .predmety__card-content {
    background-color: #ffffff;
    padding: 0px !important;
    
    display: flex;
    position: relative;
    width: 100%
  }
  .predmety__card__img-container {
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
    width: 245px !important;
    /* height: 100vh */
  }
  .predmety__card__img-container img {
    max-width: 100%;
    max-height: 100%;
    width: auto;
    height: auto;
  }
  .predmety__card__main-info {
    gap: 6px;
  }
  .predmety__card__title {
    font-family: Noto_Sans_semibold;
    font-size: 25px;
  }
  .predmety__card__info {
    margin-left: 25px;
    width: 65%;
    height: 100%;
    justify-content: space-between;
  }

  .predmety__card__extra-info {
    justify-content: space-between;
    width: 100%;
  }
  .predmety__card__extra-info p {
    width: 150px;
    color: #757670;
    width: 200px
  }

  .predmety__card__btn {
    color: white;
    background-color: #de4929;
    border-radius: 20px;
    height: 35px;
    width: 100px;
    border: none;
    margin: 10px;
    font-size: 13px;
    font-family: Noto_Sans_semibold;
  }
  .predmety__card__btn:hover {
    background-color: #e76c50;
  }
  .grid.predmety__card{
margin-top: 45px !important    ;
background-color:              #ffffff !important;
display: flex !important       ;
position: relative !important  ;
width: 100% !important         ;
flex-direction: row !important ;
left: 1vw !important;
height: 250px !important;
padding: 25px !important ;

}

  /* title and searchbar  */
  .horiz-div.predmety__title-search-fixed {
    position: fixed;
    display: flex !important;
    left: 14vw;
    top: 47px;
    background-color: #eaeae2;
    height: 165px;
    width: 50vw;
    align-items: flex-end;
    z-index: 1;
    padding-bottom: 15px;
  }
  .predmety__title {
    color: #1b1919;
    font-size: 45px;
    font-family: Noto_Sans_regular;
    word-wrap: break-word;
    margin: 0px;
    width: 470px;
  }
  .predmety__input-search__searchbar-burger {
    visibility: hidden;
    position: absolute;
  }

  p {
    margin: 0px;
  }

 #header-search-bar {
    position: fixed;
    top: 150px;
    z-index: 1000;
    right: 35vw
  }
  #header-search-bar input{
    border: 1px solid darkgray;
    background-color: #eaeae2;
    border-radius: 30px;
    height: 30px !important;
    width: 200px !important;
    padding-left: 15px;
    position: fixed;
    top: 150px;
    z-index: 1000;
    right: 35vw
  }

  .horiz-div.predmety__title-search {
    justify-content: space-between;
    width: 49vw;
  }

  /* aside part */

  .vert-div.predmety__aside {
    position: relative;
    top: 100px;
    right: -20vw;
    gap: 75px;
    height: auto;
    width: 20vw !important;
    align-content: stretch;
    display: flex !important
  }

  .predmety__aside__filter {
    gap: 20px;
  }

  .predmety__aside__dropdown-btn {
    text-decoration: none;
    font-size: 15px;
    font-family: Noto_Sans_regular;
    color: #222;
    display: flex;
    justify-content: space-between;
    border: none;
    background: none;
    width: 100%;
    text-align: left;
    cursor: pointer;
    outline: none;
    height: 40px;
    align-items: center;
  }

  .predmety__aside__dropdown-btn:hover {
    color: #9d9898;
  }

  .predmety__aside__dropdown-container {
    display: none;
    background-color: #eaeae2;
    flex-direction: column;
    color: #555555;
    padding-left: 15px;
  }

  .predmety__aside__btn {
    border: none;
    background-color: #9b9b93;
    color: white;
    height: 40px;
    display: flex;
    width: 100%;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    border: none;
    font-size: 16px;
    cursor: pointer;
    font-family: Noto_Sans_semibold;
    margin-top: 10px;
    z-index: 10
  }

  .predmety__aside__btn:hover {
    background-color: #aaaaa2;
  }

  /* form */
  .predmety__form__div__input {
    font-family: Noto_Sans_regular;
    width: 100% ;
    height: 37px;
    border: solid #555 1px;
    border-radius: 40px;
    padding: 0px 15px;
  }
  .predmety__form__div {
    width: calc(30vw - 30px);
    gap: 10px;
  }
   .horiz-div.predmety__form__div {
    width: calc(30vw - 30px);
    gap: 10px;

    align-items: flex-end;
  }
  .predmety__form__div__button-small{
     height: 35px;
    border: solid #555 1px;
    border-radius: 60px;
    background-color: white;
    font-family: Noto_Sans_regular;
    
    margin-top: 15px;
  }

  .predmety__form__div__button-small-delete{
     height: 35px;
    border: none;
    background-color: white;
    font-family: Noto_Sans_regular;
    
    margin-top: 15px;
  }
  
   .predmety__form__div__input-small {
    font-family: Noto_Sans_regular;
    height: 37px;
    border: solid #555 1px;
    border-radius: 40px;
    padding: 0px 15px;
  }
  .predmety__form__div-small{
    gap: 5px;
    width: 100%;

    align-items: flex-end;
  }
 .predmety__form__div-small__div{
    width: 40%;
    gap: 10px;

  }
  .predmety__form__div__checkbox{
    gap: 10px;
    align-items: center
  }
  
  .predmety__form {
    align-items: center;
    gap: 30px;
  }
  .predmety__form__div__input-radio {
    justify-content: space-between;
    width: 90%
  }
  .predmety__form__h {
    font-family: Noto_Sans_regular;
    font-size: 20px;
  }
  .predmety__form__div__label {
    font-family: Noto_Sans_regular;
    font-size: 14px;
    gap: 10px;
  }

  .predmety__form__div__radio {
    appearance: none;
    background-color: #fff;
    margin: 0;
    color: #333;
    width: 20px;
    height: 20px;
    border: 2px solid #333;
    border-radius: 50%;
    display: grid;
    place-content: center;
  }

  .predmety__form-main {
    margin: 0px 0px 5px;
    gap: 10px;
  }
  .predmety__form__div__radio::before {
    content: "";
    width: 8px;
    height: 8px;
    border-radius: 50%;
    transform: scale(0);
    background-color: #e0472b;
  }
  .predmety__form__div__radio:checked::before {
    transform: scale(1);
  }

  /* details page */
  /* bg */
  .details__bg {
    background-image: url("../../../../static/images/bgDetails.png");

    height: 100%;

    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    padding-top: 100px;
  }

  .details__container {
    background-color: white;
    height: auto;
    width: 70vw;
    margin: 0 auto;
    justify-content: center;
    align-items: center;
    z-index: -1;
    padding: 70px;
  }

  /* header */

  .details__div__header {
    background-color: #de4929;
    height: 40px;
    width: 68vw;
    color: white;
    justify-content: space-between;
    padding: 15px;
    margin: 0 auto;
  }
  .details__div__vr {
    width: 1px;
    border-right: 1px solid white;
    height: 20px;
    margin: 0px 10px;
  }
  .details__div__header-h {
    word-wrap: break-word;
    letter-spacing: 0.5px;
    font-size: 30px;
    font-family: Noto_Sans_regular;
  }

  /* photos */

  .details_div_images-container {
    width: 90vw;
  }
  .details_div_images {
    justify-content: space-between;
    margin-top: 30px;
  }

  .details_div_images-container__btn-left,
  .details_div_images-container__btn-right {
    visibility: hidden;
    position: absolute;
  }

  .predmety__card__img-container.details img {
    width: 13vw;
  }

  /* info */
  .details__div__details {
    justify-content: space-between;
    height: 70px;
    width: 70vw;
    margin: 60px 0px;
    align-items: flex-start;
  }
  .details__div__details-box-smaller:nth-child(odd) p {
    font-family: Noto_Sans_semibold;
  }
  .details__div__info {
    width: 70vw;
    justify-content: space-between;
    align-items: flex-start;
  }
  .details__div__info-left {
    width: 35vw;
  }
  .details__div__details-box-smaller {
    width: 10vw;
    height: 30px;
    text-align: center;
  }
  .details__div__desc p {
    margin-top: 40px;
    margin-bottom: 20px;
  }
  .details__div__info-left table tr td:nth-child(odd) {
    font-family: Noto_Sans_semibold;
    width: 15vw;
  }
  .details__div__info-left table tr td:nth-child(even) {
    width: 20vw;
  }
  .details__div__info-left table td {
    height: 45px;
    vertical-align: top;
  }
  .details__div__info-right {
    width: 30vw;
  }
  .details__div__info-right.parag {
    font-family: Noto_Sans_semibold;
    margin-bottom: 20px;
  }

  /* docs */

  .details__div__docs {
    width: 650px;
    justify-content: space-between;
  }

  .details__div__docs-files {
    width: 500px;
    justify-content: space-between;
  }
  .details__div__docs p:first-child {
    font-family: Noto_Sans_semibold;
  }

  .details__div__docs-img {
    width: 25px;
    height: auto;
    margin: 0px 10px;
  }
  a {
    color: #222;
  }

  /* additional */
  .parag {
    font-size: 17px;
    font-family: Noto_Sans_regular;
    line-height: 30px;
    word-wrap: break-word;
    letter-spacing: 0.1px;
  }
  .vr-small {
    width: 1px;
    border-right: 1px solid black;
    height: 90px;
  }
  .vr-huge-horiz {
    width: 70vw;
    border-top: 1px solid black;
    height: 1px;
    margin: 0 auto;
    margin-bottom: 30px;
    margin-top: 30px;
  }
  .vr-huge-vert {
    width: 1px;
    border-right: 1px solid black;
    height: 400px;
  }
}

