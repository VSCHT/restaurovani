.slick-prev:hover,
.slick-prev:focus,
.slick-next:hover,
.slick-next:focus {
  color: transparent;
  outline: none;
  background: transparent;
}
.slick-prev:hover:before,
.slick-prev:focus:before,
.slick-next:hover:before,
.slick-next:focus:before {
  opacity: 1;
}
.slick-prev.slick-disabled:before,
.slick-next.slick-disabled:before {
  opacity: 1;
}

.slick-prev:before,
.slick-next:before {
  font-family: "slick";
  font-size: 20px;
  line-height: 1;

  opacity: 0.75;
  color: white;

  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.slick-prev {
  left: 0px;
}
[dir="rtl"] .slick-prev {
  right: 0px;
  left: auto;
}
.slick-prev:before {
  content: url("../../../../static/images/arrow-icon-left.png");
  width: 50px
}
[dir="rtl"] .slick-prev:before {
  content: url("../../../../static/images/arrow-icon-left.png");
  width: 50px
}

.slick-next {
  right: 0px;
}
[dir="rtl"] .slick-next {
  right: auto;
  left: 0px;
}
.slick-next:before {
  content: url("../../../../static/images/arrow-icon-right.png");
  width: 50px
}
[dir="rtl"] .slick-next:before {
  content: url("../../../../static/images/arrow-icon-right.png");
  width: 50px
}

