}

/* predmety page */

/* bg */
.predmety__body-bg {
background-color:    #eaeae2;
}
/* menu part */

.dropbtn {
background-color:           #9b9b93;
color: white                ;
padding: 16px               ;
font-family: Noto_Sans_bold ;
font-size: 16px             ;
border: none                ;
}

.menu__div__dropdown {
position: relative     ;
display: inline-block  ;
}

.predmety_page.menu__btn {
visibility: hidden                              ;
position: absolute                              ;
}
.predmety__menu__btn.profil img {
width: 30px                                     ;
height: auto                                    ;
margin: 0px 10vw                                ;
}
.menu__dropdown__content {
display: none                                   ;
position: absolute                              ;
background-color:                               #f1f1f1;
min-width: 160px                                ;
