z-index: 1                                      ;
}

.menu__dropdown__content a {
color: black                 ;
padding: 12px 16px           ;
text-decoration: none        ;
display: block               ;
}

.btn.menu__btn {
font-size: 16px                ;
cursor: pointer                ;
font-family: Noto_Sans_regular ;
margin-right: 10vw             ;
}

/* title and searchbar */

.predmety__title-search__searchbar {
align-items: center !important                  ;
}
.predmety__input-search__searchbar-burger img {
width: 50px                                     ;
height: auto                                    ;
}

.horiz-div.predmety__title-search {
gap: 50px                                 ;
flex-direction: column                    ;
margin: 0 auto                            ;
}
.horiz-div.predmety__title-search-fixed {
position: fixed                           ;

top: 52px                      ;
background-color:              #eaeae2;
height: 260px                  ;
width: 100vw                   ;
align-items: flex-end          ;
z-index: 1                     ;
padding-bottom: 20px           ;
}
.predmety__title {
color:                         #1b1919;
font-size: 55px                ;
font-family: Noto_Sans_regular ;
word-wrap: break-word          ;
margin: 0px                    ;
}
/* main part--cards */
/* .predmety_main-container {
width: 100vw                   ;
position: relative             ;
top: 350px                     ;
margin: 0 auto !important      ;
} */
.predmety__cards {
width: 90vw                    ;
margin: 0 auto !important      ;
gap: 70px                      ;
}
.predmety__card {
margin: 50px auto !important   ;
background-color: white        ;
height: 300px                  ;
padding: 35px                  ;
display: flex                  ;
position: relative             ;
}

.predmety__card__title {
font-family: Noto_Sans_regular ;
font-size: 4vw                 ;
width: auto                    ;
line-height:27px
}
.description.parag{
font-size: 2vw                 ;
}
p {
margin: 0px                    ;
}

.predmety__card__info {
margin-left: 30px       ;

height: 80%                    ;
justify-content: space-between ;
width: 45vw                    ;
}

.predmety__card__img-container {
display: flex                        ;
align-items: center                  ;
justify-content: center              ;
overflow: hidden                     ;
width: 305px                         ;
}
.predmety__card__img-container img {
max-width: 100%                      ;
max-height: 100%                     ;
width: 240px                         ;
height: auto                         ;
}

.predmety__card__main-info {
gap: 20px                    ;
}

.predmety__card__btn {
color: white                ;
background-color:           #de4929;
border-radius: 40px         ;
height: 60px                ;
width: 210px                ;
border: none                ;
margin: 10px                ;
