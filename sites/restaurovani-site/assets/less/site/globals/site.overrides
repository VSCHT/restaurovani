}
.predmety__form__div-fields{
width: 100%                         ;
gap: 35px                           ;
}
.predmety__form__div-unit{
width: calc(20vw - 30px)            ;
}
.predmety__form {
align-items: center                 ;
gap: 25px                           ;
}
.predmety__form__div__input-radio {
justify-content: space-between      ;
column-gap: 85px                    ;
flex-wrap: wrap                     ;
width: 90%                          ;
row-gap:20px
}
.predmety__form__div__radio {
appearance: none                    ;
background-color:                   #fff;
margin: 0                           ;
color:                              #333;
width: 20px                         ;
height: 20px                        ;
border: 2px solid                   #333;
border-radius: 50%                  ;
display: grid                       ;
place-content: center               ;
}

.predmety__form-main {
margin: 0px 0px 5px                           ;
gap: 20px                                     ;
max-width: 60vw                               ;
}
.form.main-page__btn__addPredmety {
width: 250px                                  ;
