}

.predmety__card__extra-info {
justify-content: space-between ;
width: 100%                    ;
}

.predmety__card__extra-info p {
width: 175px                    ;
font-size: 2vw                  ;
}

/* aside part */
.vert-div.predmety__aside {
position: absolute !important ;
gap: 60px                     ;
height: auto                  ;
width: 100vw !important       ;
