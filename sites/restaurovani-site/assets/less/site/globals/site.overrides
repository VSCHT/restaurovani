height: 60px                              ;
}
.predmety__form__div__input {
font-family: Noto_Sans_regular            ;
width: calc(100% - 20px)                  ;
height: 35px                              ;
border: solid                             #555 1px;
border-radius: 40px                       ;
padding: 0px 10px                         ;
}

.predmety__form__div {
width: 100%                         ;
gap: 20px                           ;
height: 61px                        ;
}
.predmety__form__div-checkbox {
width: 100%                         ;
gap: 13px                           ;
height: 30px                        ;
}
.predmety__form__div-dimensions{
width: 100%                         ;
gap: 20px                           ;
