.ui.unstackable.table {
  text-align:center;
  
  & tr {
    display: flex;
    flex-flow: column;
    border-left: 1px solid @brownColor;
  }
  & tbody {
    display: flex;
  }
  @media only screen and (max-width: @mobileSize) {
    & tr {
      display: flex;
      flex-flow: row;
    }
    & tbody {
      flex-direction: column;
    }
  }
}



